{
  "echo": {
    "prefix": "echo",
    "body": [
      "<%= $1 %>"
    ]
  },
  "erb": {
    "prefix": "erb",
    "body": [
      "<% $1 %>"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "<% if $1 %>",
      "  $2",
      "<% end %>"
    ]
  },
  "ife": {
    "prefix": "ife",
    "body": [
      "<% if $1 %>",
      "  $2",
      "<% else %>",
      "  $3",
      "<% end %>"
    ]
  },
  "raise": {
    "prefix": "raise",
    "body": [
      "<% raise %>"
    ]
  },
  "collection": {
    "prefix": "ea",
    "body": [
      "<% $1.each do |$2| %>",
      "  <%= $3 %>",
      "<% end %>"
    ]
  },
  "end": {
    "prefix": "end",
    "body": [
      "<% end %>"
    ]
  },
  "link": {
    "prefix": "link",
    "body": [
      "<%= link_to \"$1\", $2 %>"
    ]
  },
  "turbo_frame": {
    "prefix": "turbo_frame",
    "body": [
      "<%= turbo_frame_tag \"$1\" do %>",
      "  $2",
      "<% end %>"
    ]
  },
  "bootstrap_table": {
    "prefix": "bs_table",
    "body": [
      "<table class=\"table table-striped\">",
      "  <thead>",
      "    <th></th>",
      "  </thead>",
      "  <tbody>",
      "    <% $1.each do |$2| %>",
      "      <tr>",
      "        <td></td>",
      "      </tr>",
      "    <% end %>",
      "  </tbody>",
      "<% end %>"
    ]
  }
}
