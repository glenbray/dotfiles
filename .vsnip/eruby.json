{
  "echo": {
    "prefix": "echo",
    "body": [
      "<%= $1 %>"
    ]
  },
  "erb": {
    "prefix": "erb",
    "body": [
      "<% $1 %>"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "<% if $1 %>",
      "<% end %>"
    ]
  },
  "raise": {
    "prefix": "raise",
    "body": [
      "<% raise %>"
    ]
  },
  "collection": {
    "prefix": "ea",
    "body": [
      "<% $1.each do |$2| %>",
      "  <%= $3 %>",
      "<% end %>"
    ]
  },
  "end": {
    "prefix": "end",
    "body": [
      "<% end %>"
    ]
  },
  "link": {
    "prefix": "link",
    "body": [
      "<%= link_to '$1', $2 %>"
    ]
  }
}
